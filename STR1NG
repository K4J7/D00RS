local function loadScript(url)
    loadstring(game:HttpGet(url))()
end
 
-- Define the URLs
local url1 = 'loadstring(game:HttpGet("https://raw.githubusercontent.com/WereHappyTogether/d00r5/refs/heads/main/m41n"))()'
local url2 = 'https://raw.githubusercontent.com/K4J7/D00RS/refs/heads/main/LISTENER'

 
-- Create coroutines for each loadstring
local coroutine1 = coroutine.create(function()
    loadScript(url1)
end)
 
local coroutine2 = coroutine.create(function()
    loadScript(url2)
end)

local coroutine3 = coroutine.create(function()

loadScript(url3)
end)
 
-- Start both coroutines
coroutine.resume(coroutine1)
coroutine.resume(coroutine2)
coroutine.resume(coroutine3)
